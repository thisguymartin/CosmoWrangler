{
  "name": "search-hound-js",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .js .",
    "commit": "cz"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@types/node": "^18.16.0",
    "commander": "^10.0.1",
    "eslint": "^8.39.0",
    "figlet": "^1.6.0",
    "langchain": "^0.0.63",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.5",
    "@vitest/coverage-c8": "^0.30.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "vite": "^4.3.1",
    "vitest": "^0.30.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
